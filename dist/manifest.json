{
  "name": "zeel",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "time pass",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "http://*/*", 
    "https://*/*"
  ],
  "icons": {
      "512": "logo512.png"
  },
  "browser_action": {
      "default_icon": "logo512.png",
      "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self'; default-src 'self'",

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["./script/contentScript.js"]
    }
  ]

}